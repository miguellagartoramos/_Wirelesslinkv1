@Code
    Layout = "~/Views/Shared/_Layout.vbhtml"
    ViewData("Title") = "Desk Organizer"
End Code
<section class="ubiquiti-products">
    <div class="products">
        <!--<img src="~/img/Ubiquiti/ubnt-banner.jpg" class="img-responsive">-->
        <div class="product-list">
            <div class="parallax-container hidden-xs" style=" background: url('/img/box-texture.png')">
                <div class="parallax"><img src="~/img/desk-organizer-parallax.jpg" alt="cable organiser" ></div>
            </div>

            <div class="container">
                <div class="product-item">
                    <ol class="breadcrumb">
                        <li><a href="~/Home/Index">Home</a></li>
                        <li><a class="active" href="~/DeskOrganizer">Desk Organizer</a></li>
                    </ol>
                </div>
                <h4>Desk Organizer</h4>
                <p>
                    Desks are one of the easiest places for cords and cables to get cluttered, tangled, and altogether unorganized. So how can you stay plugged in without the wires taking over? Fortunately, we just so happen to have a huge range of cable management ideas and products designed specifically to keep your desk cables in check.
                </p>

            </div>

            <div class="container">
                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.2s">
                            <a href="~/DeskOrganizer/desk_cable_management"></a>
                            <img src="~/img/desk_organizer/desk_cable_management.jpg" class="img-responsive" alt="Desk Cable Management" title="Desk Cable Management">
                            <span class="prod-name">Desk Cable Management</span>
                            <span class="prod-desc">Everything you need for your home & office desk cables</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.4s">
                            <a href="~/DeskOrganizer/mounts_and_kits"></a>
                            <img src="~/img/desk_organizer/computer_mount.jpg" class="img-responsive" alt="Computer Mounts & Kits" title="Computer Mounts & Kits">
                            <span class="prod-name">Computer Mounts & Kits</span>
                            <span class="prod-desc">Wall-mounts, CPU holders & theft prevention kits</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

                    <div class="clearfix col-sm-12 visible-sm"></div>

                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.6s">
                            <a href="~/DeskOrganizer/charging_station_and_batteries"></a>
                            <img src="~/img/desk_organizer/chargin_station.jpg" class="img-responsive" alt="Charging Stations & Batteries" title="Charging Stations & Batteries">
                            <span class="prod-name">Charging Stations & Batteries</span>
                            <span class="prod-desc">USB chargers, cables, plugs, charging stations, solar & more</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.8s">
                            <a href="~/DeskOrganizer/computer_workstation"></a>
                            <img src="~/img/desk_organizer/computer_workstation.jpg" class="img-responsive" alt="Computer Table" title="Computer Workstations">
                            <span class="prod-name">Computer Workstations</span>
                            <span class="prod-desc">Desks, mobile carts, storage cabinets & accessories</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

                    <div class="clearfix col-lg-12 visible-lg"></div>

                    <div class="clearfix col-md-12 visible-md"></div>

                    <div class="clearfix col-sm-12 visible-sm"></div>

                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.2s">
                            <a href="~/DeskOrganizer/mobile_and_tablet_accessories"></a>
                            <img src="~/img/desk_organizer/mobile_tablet_accessories.jpg" class="img-responsive" alt="Mobile & Tablet Accessories" title="Mobile & Tablet Accessories">
                            <span class="prod-name">Mobile & Tablet Accessories</span>
                            <span class="prod-desc">Stylish stands & other solutions to keep your desk organized</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.4s">
                            <a href="~/DeskOrganizer/wraps_clips_clamps"></a>
                            <img src="~/img/cable-management/wraps_clips_clamp.jpg" class="img-responsive" alt="Wraps, Clips & Clamps" title="Wraps, Clips & Clamps">
                            <span class="prod-name">Wraps, Clips & Clamps</span>
                            <span class="prod-desc">Several solutions for bundling, routing, & protecting your cables</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.6s">
                            <a href="~/DeskOrganizer/velco_wraps"></a>
                            <img src="~/img/cable-management/velcro_wraps.jpg" class="img-responsive" alt="Velcro Wraps & Fasteners" title="Velcro Wraps & Fasteners">
                            <span class="prod-name">Velcro Wraps & Fasteners</span>
                            <span class="prod-desc">Straps & wraps for organizing, managing & bundling your cords</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-6">
                        <div class="product-info wow fadeInUp" data-wow-delay="0.8s">
                            <a href="~/DeskOrganizer/cable_identification_tags"></a>
                            <img src="~/img/desk_organizer/cable_indentification_tags.jpg" class="img-responsive" alt="Cable Identification Tags" title="Cable Identification Tags">
                            <span class="prod-name">Cable Identification Tags</span>
                            <span class="prod-desc">Keep track of which cable is which with ties & tags</span>
                            <span class="prod-tag">View Details<i class="material-icons pull-right hidden-xs">trending_flat</i></span>
                        </div>
                    </div>

            </div>

        </div>


    </div>
</section>


